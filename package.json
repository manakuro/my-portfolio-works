{
  "name": "my-portfolio-works-new",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@types/classnames": "^2.2.3",
    "@types/react-transition-group": "^2.0.9",
    "node-sass-chokidar": "^0.0.3",
    "npm-run-all": "^4.1.2",
    "react": "^16.2.0",
    "react-anime": "^2.1.0",
    "react-dom": "^16.2.0",
    "react-scripts-ts": "2.13.0"
  },
  "scripts": {
    "start": "npm-run-all -p watch-css start-js",
    "build": "npm-run-all build-css build-js",
    "start-js": "react-scripts-ts start",
    "build-js": "react-scripts-ts build",
    "build-css":
      "node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/",
    "watch-css":
      "npm run build-css && node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/ --watch --recursive",
    "test": "react-scripts-ts test --env=jsdom",
    "eject": "react-scripts-ts eject",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "*.js": [
      "prettier --single-quote --trailing-comma all --write",
      "eslint --fix",
      "git add"
    ],
    "*.tsx": [
      "prettier --single-quote --trailing-comma all --write",
      "tslint --fix",
      "git add"
    ],
    "*.graphql": ["prettier --write", "git add"],
    "*.json": ["prettier --write", "git add"]
  },
  "devDependencies": {
    "@types/jest": "^22.1.2",
    "@types/node": "^9.4.5",
    "@types/react": "^16.0.36",
    "@types/react-dom": "^16.0.3",
    "@types/react-redux": "^5.0.14",
    "animate.css": "^3.6.1",
    "classnames": "^2.2.5",
    "eslint": "^4.17.0",
    "font-awesome": "^4.7.0",
    "github-markdown-css": "^2.10.0",
    "husky": "^0.14.3",
    "lint-staged": "^6.1.0",
    "prettier": "^1.10.2",
    "react-animated-css": "^1.0.4",
    "react-canvas": "^1.3.0",
    "react-id-swiper": "^1.6.1",
    "react-markdown": "^3.3.0",
    "react-redux": "^5.0.6",
    "react-show": "^2.0.4",
    "react-transition-group": "^2.3.0",
    "redux": "^3.7.2",
    "sanitize.css": "^5.0.0",
    "tslint-config-prettier": "^1.7.0",
    "typescript": "^2.7.1"
  },
  "engines": {
    "node": "9.4.0",
    "npm": "*",
    "yarn": "*"
  }
}
